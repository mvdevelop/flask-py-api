{
  "version": 2,
  "name": "pystore-api",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "PYTHONPATH": "/var/task:/var/task/app"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "regions": ["gru1"]
}
